{
    "name": "vimex/batch-bundle",
    "type": "symfony-bundle",
    "description": "Akeneo Symfony Batch Bundle",
    "keywords": ["Akeneo", "Batch", "Job", "Reader", "Writer", "Processor"],
    "homepage": "https://github.com/akeneo/BatchBundle",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Akeneo",
            "homepage": "http://www.akeneo.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "vimex/storage-utils-bundle": "dev-symfony-5",
        "vimex/batch": "dev-symfony-5",
        "symfony/config": "^5.4",
        "symfony/console": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/event-dispatcher": "^5.4",
        "symfony/http-kernel": "^5.4",
        "symfony/translation": "^5.4",
        "symfony/process": "^5.4",
        "symfony/security-bundle": "^5.4",
        "symfony/validator": "^5.4",
        "symfony/framework-bundle": "^5.4",
        "doctrine/orm": "*",
        "monolog/monolog": "*",
        "twig/extensions": "*",
        "symfony/swiftmailer-bundle": "*"
