    "autoload": {
        "psr-4": { "Akeneo\\Bundle\\BatchBundle\\": "" }
    },
    "repositories": [
        {
            "url": "https://github.com/virgileb/StorageUtilsBundle",
            "type": "git"
        },
        {
            "url": "https://github.com/virgileb/StorageUtils",
            "type": "git"
